{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@mdi/js": "^6.1.95",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/button": "^5.0.1",
    "@smui/checkbox": "^5.0.1",
    "@smui/common": "^5.0.1",
    "@smui/data-table": "^5.0.1",
    "@smui/dialog": "^5.0.1",
    "@smui/drawer": "^5.0.1",
    "@smui/fab": "^5.0.1",
    "@smui/form-field": "^5.0.1",
    "@smui/icon-button": "^5.0.1",
    "@smui/layout-grid": "^5.0.1",
    "@smui/list": "^5.0.1",
    "@smui/radio": "^5.0.1",
    "@smui/select": "^5.0.1",
    "@smui/snackbar": "^5.0.1",
    "@smui/textfield": "^5.0.1",
    "@tsconfig/svelte": "^2.0.0",
    "axios": "^0.21.4",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.39.1",
    "smui-theme": "^5.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-navigator": "^3.1.5"
  }
}
